/* components/pagination/pagination.wxss */
.pagination{
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: center;
}
.pagination button{
  background: #48dbfb;
  margin: 5rpx;
}
.pagination .prev{
  flex: 3;
  text-align: center;
}
.pagination .pagenum{
  flex: 1;
  text-align: center;
  line-height: 80rpx;
}

.pagination .next{
  flex: 3;
  text-align: center;
}